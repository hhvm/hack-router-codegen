{
    "name": "facebook/hack-router-codegen",
    "description": "URI routing for Hack with codegen",
    "keywords": ["hack", "router", "routing", "hhvm"],
    "homepage": "https://github.com/hhvm/hack-router-codegen",
    "require": {
        "hhvm": "^4.93",
        "facebook/hack-router": "^0.19",
        "facebook/hack-http-request-response-interfaces": "^0.2|^0.3",
        "facebook/hack-codegen": "^4.5.0",
        "facebook/definition-finder": "^2.7",
        "hhvm/type-assert": "^3.0|^4.0"
    },
    "require-dev": {
        "hhvm/hhast": "^4.6",
        "hhvm/hhvm-autoload": "^2.0|^3.0",
        "hhvm/hacktest": "^2.0",
        "facebook/fbexpect": "^2.6.1",
        "usox/hackttp": "^0.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "autoload": {
      "classmap": [ "src/" ]
    },
    "autoload-dev": {
      "classmap": [ "tests/" ]
    }
}
