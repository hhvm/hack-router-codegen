{
    "name": "facebook/hack-router-codegen",
    "description": "URI routing for Hack with codegen",
    "keywords": ["hack", "router", "routing", "hhvm"],
    "homepage": "https://github.com/hhvm/hack-router-codegen",
    "require": {
        "hhvm": "^3.25.0",
        "facebook/hack-router": ">=0.14",
        "facebook/hack-codegen": "^3.0.1",
        "facebook/definition-finder": "^2.0",
        "hhvm/type-assert": "^3.0"
    },
    "require-dev": {
        "91carriage/phpunit-hhi": "^5.5",
        "phpunit/phpunit": "^5.5",
        "hhvm/hhast": "^3.26"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
      "classmap": [ "src/" ]
    },
    "autoload-dev": {
      "classmap": [ "tests/" ]
    }
}
